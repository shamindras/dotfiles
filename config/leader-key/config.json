    {
      "actions": [
        {
          "key": "1",
          "label": "1password",
          "type": "command",
          "value": "osascript -e 'quit app \"1Password\"' && sleep 0.1 && /bin/zsh -c '/opt/homebrew/bin/aerospace workspace B'"
        },
        {
          "key": "a",
          "label": "djview",
          "type": "command",
          "value": "(pkill -x djview 2>/dev/null || true) && sleep 0.1 && /bin/zsh -c '/opt/homebrew/bin/aerospace workspace A'"
        },
        {
          "key": "b",
          "label": "firefox",
          "type": "command",
          "value": "osascript -e 'quit app \"Firefox\"' && sleep 0.1 && /bin/zsh -c '/opt/homebrew/bin/aerospace workspace B'"
        },
        {
          "key": "d",
          "label": "preview",
          "type": "command",
          "value": "osascript -e 'quit app \"Preview\"' && sleep 0.1 && /bin/zsh -c '/opt/homebrew/bin/aerospace workspace B'"
        },
        {
          "key": "e",
          "label": "finder",
          "type": "command",
          "value": "osascript -e 'tell application \"Finder\" to activate' && sleep 0.2 && osascript -e 'tell application \"System Events\" to keystroke \"q\" using command down' && sleep 0.2 && /bin/zsh -c '/opt/homebrew/bin/aerospace workspace B'"
        },
        {
          "key": "g",
          "label": "signal",
          "type": "command",
          "value": "osascript -e 'quit app \"Signal\"' && sleep 0.1 && /bin/zsh -c '/opt/homebrew/bin/aerospace workspace B'"
        },
        {
          "key": "i",
          "label": "books",
          "type": "command",
          "value": "(pkill -x Books 2>/dev/null || true) && sleep 0.1 && /bin/zsh -c '/opt/homebrew/bin/aerospace workspace B'"
        },
        {
          "key": "m",
          "label": "spotify",
          "type": "command",
          "value": "osascript -e 'quit app \"Spotify\"' && sleep 0.1 && /bin/zsh -c '/opt/homebrew/bin/aerospace workspace B'"
        },
        {
          "key": "n",
          "label": "nordvpn",
          "type": "command",
          "value": "osascript -e 'quit app \"NordVPN\"' && sleep 0.1 && /bin/zsh -c '/opt/homebrew/bin/aerospace workspace B'"
        },
        {
          "key": "p",
          "label": "skim",
          "type": "command",
          "value": "osascript -e 'quit app \"Skim\"' && sleep 0.1 && if pgrep -x \"ghostty\" > /dev/null; then /bin/zsh -c '/opt/homebrew/bin/aerospace workspace T'; else /bin/zsh -c '/opt/homebrew/bin/aerospace workspace B'; fi"
        },
        {
          "key": "t",
          "label": "ghostty",
          "type": "command",
          "value": "osascript -e 'quit app \"Ghostty\"' && sleep 0.1 && /bin/zsh -c '/opt/homebrew/bin/aerospace workspace B'"
        },
        {
          "key": "v",
          "label": "vscode",
          "type": "command",
          "value": "osascript -e 'quit app \"Code\"' && sleep 0.1 && /bin/zsh -c '/opt/homebrew/bin/aerospace workspace B'"
        },
        {
          "key": "w",
          "label": "wezterm",
          "type": "command",
          "value": "osascript -e 'quit app \"WezTerm\"' && sleep 0.1 && if pgrep -x \"ghostty\" > /dev/null; then /bin/zsh -c '/opt/homebrew/bin/aerospace workspace T'; else /bin/zsh -c '/opt/homebrew/bin/aerospace workspace B'; fi"
        },
        {
          "key": "x",
          "label": "jdownloader",
          "type": "command",
          "value": "(pkill -f \"JDownloader2.app\" 2>/dev/null || true) && sleep 0.1 && /bin/zsh -c '/opt/homebrew/bin/aerospace workspace B'"
        }
      ],
      "key": "q",
      "label": "quit",
      "type": "group"
    },
    {
      "actions": [
        {
          "key": "m",
          "label": "move-files",
          "type": "command",
          "value": "find ~/Downloads -type f \\( -iname \"*.pdf\" -o -iname \"*.djvu\" \\) -exec mv {} /Users/shamindras/DROPBOX/RESOURCES/BOOKS/REFERENCE_BOOKS \\; ; find ~/Downloads -type f -iname \"*.epub\" -exec mv {} /Users/shamindras/DROPBOX/RESOURCES/BOOKS/EBOOKS \\; ; find /Users/shamindras/DROPBOX/RESOURCES/BOOKS/REFERENCE_BOOKS -type f -iname \"*.epub\" -exec mv {} /Users/shamindras/DROPBOX/RESOURCES/BOOKS/EBOOKS \\;"
        },
        {
          "key": "r",
          "label": "restart-leaderkey",
          "type": "command",
          "value": "killall 'Leader Key' 2>/dev/null; sleep 0.5; open -a 'Leader Key'"
        },
        {
          "key": "t",
          "label": "trash-empty",
          "type": "command",
          "value": "osascript -e 'tell application \"Finder\" to activate' -e 'tell application \"Finder\" to if (count of items in trash) > 0 then empty trash' && sleep 0.2 && (killall Finder 2>/dev/null || true) && sleep 0.1 && /bin/zsh -c '/opt/homebrew/bin/aerospace workspace B'"
        },
        {
          "key": "u",
          "label": "toggle-mute",
          "type": "command",
          "value": "osascript -e 'set curVolume to get volume settings' -e 'if output muted of curVolume is false then' -e 'set volume with output muted' -e 'else' -e 'set volume without output muted' -e 'end if'"
        },
        {
          "key": "w",
          "label": "wash-dropbox",
          "type": "command",
          "value": "find ~/DROPBOX/RESOURCES/BOOKS ~/Downloads -type f -name \".DS_Store\" -print0 | xargs -0 -P $(sysctl -n hw.ncpu) rm"
        }
      ],
      "key": "r",
      "label": "run",
      "type": "group"
    },
